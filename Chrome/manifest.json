{
  "manifest_version": 2,
  "name": "No More Mobile",
  "description": "__MSG_appDesc__",
  "version": "1.1",
  "default_locale": "en",
  "options_page": "options.html",
  "background":
  {
    "scripts": ["background.js"]
  },
  "icons":
  {
    "128": "icon.png"
  },
  "permissions":
  [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}