{
  "manifest_version": 2,
  "name": "No More Mobile",
  "description": "Redirects mobile pages to classic (desktop) pages",
  "version": "0.1",
  "background":
  {
    "scripts": ["background.js"]
  },
  "icons":
  {
    "128": "icon.png"
  },
  "permissions":
  [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}